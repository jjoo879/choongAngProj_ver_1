<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.S20230403.notice_title">
	<select id="ysNotifaq" resultType="notice_title">
		SELECT
		*
		FROM
		notice_faq
		where notice_type =410
	</select>
	<select id="yslistnc" resultType="notice_title">
		SELECT
		notice_content
		FROM
		NOTICE_FAQ
	</select>
	<select id="yslistnt" resultType="notice_title">
		SELECT
		notice_title
		FROM
		NOTICE_FAQ
	</select>
</mapper>